<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorFondo2"
    tools:context=".presentation.profile.Profile">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="372dp"
                    android:layout_height="751dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintLayout"
                        android:layout_width="374dp"
                        android:layout_height="149dp"
                        android:background="@drawable/rectangle_66"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/imageView6"
                        android:layout_width="124dp"
                        android:layout_height="129dp"
                        android:layout_marginTop="104dp"
                        android:scaleType="centerCrop"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
                        app:shapeAppearanceOverlay="@style/rounded"
                        app:srcCompat="@drawable/logo"
                        app:strokeColor="@color/colorBlanco"
                        app:strokeWidth="4dp"
                        tools:ignore="MissingConstraints" />

                    <TextView
                        android:id="@+id/textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Paqta ADM"
                        android:textSize="20dp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/imageView6"
                        tools:ignore="MissingConstraints" />

                    <LinearLayout
                        android:id="@+id/linearLayout"
                        android:layout_width="329dp"
                        android:layout_height="336dp"
                        android:layout_marginStart="24dp"
                        android:layout_marginTop="24dp"
                        android:layout_marginEnd="24dp"
                        android:background="@drawable/whitebck"
                        android:orientation="vertical"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="339dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="20dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/txtView32"
                                    android:layout_width="131dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="Rol:"
                                    android:textColor="#FFFFFF"
                                    android:textSize="20sp" />


                                <TextView
                                    android:id="@+id/textView6"
                                    android:layout_width="168dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="Administrador"
                                    android:textColor="#6E6666"
                                    android:textSize="20sp" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="20dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/textView4"
                                    android:layout_width="131dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="Nombre:"
                                    android:textColor="#FFFFFF"
                                    android:textSize="20sp" />


                                <TextView
                                    android:id="@+id/textView5"
                                    android:layout_width="168dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="Paqta ADM"
                                    android:textColor="#6E6666"
                                    android:textSize="20sp" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="20dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/txtView31"
                                    android:layout_width="131dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="Direccion:"
                                    android:textColor="#FFFFFF"
                                    android:textSize="20sp" />


                                <TextView
                                    android:id="@+id/txtView33"
                                    android:layout_width="168dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="@string/labelCampoNulo"
                                    android:textColor="#6E6666"
                                    android:textSize="20sp" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="295dp"
                                android:layout_height="wrap_content"
                                android:layout_margin="20dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/txtView34"
                                    android:layout_width="131dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="Telefono:"
                                    android:textColor="#FFFFFF"
                                    android:textSize="20sp" />


                                <TextView
                                    android:id="@+id/txtView35"
                                    android:layout_width="168dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="@string/labelCampoNulo"
                                    android:textColor="#6E6666"
                                    android:textSize="20sp" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="20dp"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/textView2"
                                    android:layout_width="131dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="DNI:"
                                    android:textColor="#FFFFFF"
                                    android:textSize="20sp" />


                                <TextView
                                    android:id="@+id/textView3"
                                    android:layout_width="168dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/archivo"
                                    android:text="@string/labelCampoNulo"
                                    android:textColor="#6E6666"
                                    android:textSize="20sp" />

                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>

                    <Button
                        android:id="@+id/button2"
                        android:layout_width="158dp"
                        android:layout_height="48dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="12dp"
                        android:text="EDITAR"
                        android:backgroundTint="@color/colorFondo"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

                    <Button
                        android:id="@+id/button4"
                        android:layout_width="331dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="60dp"
                        android:backgroundTint="@color/colorRojo"
                        android:text="CERRAR SESION"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

                    <Button
                        android:id="@+id/button3"
                        android:layout_width="158dp"
                        android:layout_height="48dp"
                        android:layout_marginTop="12dp"
                        android:layout_marginEnd="24dp"
                        android:backgroundTint="@color/colorFondo"
                        android:text="ADMIN"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>